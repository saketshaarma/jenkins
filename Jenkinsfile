pipeline {
    agent any
    stages {
        stage('---clean---'){
            steps {
                echo "Hello India"
            }
        }
        stage('---Build---'){
          steps {
              echo "It is my country"
          }
        }
        stage('---deployment---'){
          steps {
            echo "I love my country"
          }
        }
        stage('---Completion---'){
          steps {
            echo "I love its people"
          }
        }
    }
}
