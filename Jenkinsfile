pipeline{
agent any
  stages{
    stage('----clean----'){
      steps{
        echo "Hello India"
  }
  }
  }
  stages{
    stage('---Build---'){
      steps{
        echo "This is my country"
  }
  }

}
  stages{
    stage('---deployment---'){
      steps{
        echo "I love my country"
  }
  }
}
}
